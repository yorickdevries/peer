{
  "name": "ed3_peer_review",
  "version": "1.0.0",
  "scripts": {
    "install": "npm install --prefix ./client && npm install --prefix ./server",
    "start": "npm run build && npm run start_server",
    "build": "npm run build_server && npm run build_db && npm run build_client",
    "build_server": "npm run build --prefix ./server",
    "build_db": "npm run db_init --prefix ./server",
    "build_client": "npm run build --prefix ./client",
    "start_server": "npm run start_server --prefix ./server",
  }
}
