{
  "name": "peer_review",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./dist/bin/www.js",
    "tslint": "tslint -- src/**/*.ts test/**/*.ts",
    "build": "npm run tsc",
    "tsc": "tsc",
    "test": "nyc mocha -r ts-node/register test/**/*.spec.ts"
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text",
      "text-summary"
    ]
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "pg-promise": "8.4.4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.3",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/http-errors": "^1.6.1",
    "@types/jade": "0.0.30",
    "@types/mocha": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.0.8",
    "@types/pg-promise": "^5.4.3",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "nyc": "^11.7.3",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3"
  }
}
