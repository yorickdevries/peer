{
  "name": "peer_review_server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run start_server",
    "dev": "nodemon",
    "start_newdata": "npm run build && npm run data_init && npm run start_server",
    "start_server": "node ./dist/bin/www.js",
    "tslint": "tslint --project ./tsconfig_tslint.json",
    "build": "tsc",
    "test": "mocha",
    "coverage": "nyc npm run test",
    "data_init": "node ./dist/scripts/data_init.js"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 50,
    "all": true,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/bin/www.ts",
      "src/scripts/*.ts"
    ],
    "reporter": [
      "text",
      "text-summary"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/helmet": "0.0.46",
    "@types/is-ci": "^1.1.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/node": "^10.17.21",
    "@types/passport": "^0.4.6",
    "@types/passport-saml": "^0.15.7",
    "@types/session-file-store": "^1.2.0",
    "bluebird": "3.5.1",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "export-from-json": "^1.1.4",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.15.6",
    "file-system-cache": "^1.0.5",
    "fs-extra": "^6.0.1",
    "helmet": "^3.22.0",
    "is-ci": "^1.1.0",
    "json2csv": "^4.5.2",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "neat-csv": "^2.1.0",
    "passport": "^0.4.0",
    "passport-mock-strategy": "^1.1.1",
    "passport-saml": "^0.35.0",
    "passport-saml-metadata": "^1.5.1",
    "pg-promise": "8.4.4",
    "session-file-store": "^1.2.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-http": "^3.0.4",
    "@types/mocha": "^5.2.0",
    "@types/mockdate": "^2.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.0.0",
    "mocha": "^5.1.1",
    "mockdate": "^2.0.2",
    "nodemon": "^2.0.3",
    "nyc": "^11.7.3",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.2.0",
    "tslint": "^5.10.0"
  }
}
