{
  "name": "peer_review",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run db_init && npm run start_server",
    "start_server": "node ./dist/bin/www.js",
    "tslint": "tslint --project ./tsconfig_tslint.json",
    "build": "npm run tsc",
    "tsc": "tsc",
    "test": "mocha --timeout 10000",
    "coverage": "nyc npm run test",
    "db_init": "node ./dist/scripts/database_init.js"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "all": true,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/bin/www.ts",
      "src/scripts/*.ts"
    ],
    "reporter": [
      "text",
      "text-summary"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@okta/oidc-middleware": "^0.1.2",
    "bluebird": "3.5.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "fs-extra": "^6.0.1",
    "http-errors": "~1.6.2",
    "is-ci": "^1.1.0",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "neat-csv": "^2.1.0",
    "pg-promise": "8.4.4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.3",
    "@types/chai-http": "^3.0.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/http-errors": "^1.6.1",
    "@types/is-ci": "^1.1.0",
    "@types/jade": "0.0.30",
    "@types/mocha": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/node": "^10.0.8",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.1.1",
    "nyc": "^11.7.3",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
