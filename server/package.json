{
  "name": "peer_review_server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run start_server",
    "dev": "nodemon",
    "start_newdata": "npm run build && npm run data_init && npm run start_server",
    "start_server": "node ./dist/bin/www.js",
    "tslint": "tslint --project ./tsconfig_tslint.json",
    "build": "tsc",
    "test": "mocha",
    "coverage": "nyc npm run test",
    "data_init": "node ./dist/scripts/data_init.js"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 50,
    "all": true,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/bin/www.ts",
      "src/scripts/*.ts"
    ],
    "reporter": [
      "text",
      "text-summary"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.47",
    "@types/is-ci": "^1.1.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.3",
    "@types/session-file-store": "^1.2.1",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "debug": "^2.6.9",
    "export-from-json": "^1.3.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "file-system-cache": "^1.0.5",
    "forwarded-for": "^1.1.0",
    "fs-extra": "^6.0.1",
    "helmet": "^3.22.0",
    "is-ci": "^1.2.1",
    "json2csv": "^4.5.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "neat-csv": "^5.2.0",
    "passport": "^0.4.1",
    "passport-mock-strategy": "^1.1.1",
    "passport-saml": "^0.35.0",
    "passport-saml-metadata": "^1.6.0",
    "pg-promise": "^10.5.6",
    "session-file-store": "^1.4.0",
    "strip-bom-buf": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "mocha": "^5.2.0",
    "mockdate": "^3.0.2",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^6.2.0",
    "tslint": "^6.1.2"
  }
}
