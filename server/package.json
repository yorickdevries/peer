{
  "name": "peer_review_server",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/bin/www.js",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "lint-fix-tests": "eslint . --ext .test.ts --fix",
    "test": "jest --runInBand ./tests",
    "test2": "jest --runInBand ./tests/e2e/academicyears.test.ts",
    "coverage": "jest --runInBand --collectCoverage ./tests",
    "createdata": "ts-node src/scripts/createData.ts",
    "initdata": "node ./dist/scripts/initializeData.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migration:generate": "npm run typeorm migration:generate -- -n",
    "typeorm:migration:run": "npm run typeorm migration:run",
    "typeorm:migration:revert": "npm run typeorm migration:revert",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js -r 'src/../../src' config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n ormconfig.ts -r 'src/../../src'",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js --config src/ormconfig.ts schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js --config src/ormconfig.ts schema:sync",
    "seed": "npm run schema:drop && npm run typeorm:migration:run && npm run seed:run"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@sentry/node": "^7.47.0",
    "@sentry/tracing": "^7.47.0",
    "@types/compression": "^1.7.2",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express-session": "^1.17.7",
    "@types/hapi__joi": "^17.1.9",
    "@types/helmet": "0.0.48",
    "@types/is-ci": "^3.0.0",
    "@types/json2csv": "^5.0.3",
    "@types/lodash": "^4.14.192",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.42",
    "@types/node-schedule": "^1.3.2",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-saml": "^1.1.3",
    "@types/passport-saml-metadata": "^2.2.1",
    "@types/session-file-store": "^1.2.2",
    "@types/workerpool": "^6.4.0",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "csv-parse": "^5.3.6",
    "csv-string": "^4.1.1",
    "detect-ts-node": "^1.0.5",
    "dist-exiftool": "^10.53.0",
    "export-from-json": "^1.7.2",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "file-system-cache": "^2.0.2",
    "forwarded-for": "^1.1.0",
    "helmet": "^6.0.1",
    "is-ci": "^3.0.1",
    "js-graph-algorithms": "^1.0.18",
    "json-parse-even-better-errors": "^2.3.1",
    "json2csv": "^5.0.7",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "neat-csv": "^5.2.0",
    "node-exiftool": "^2.3.0",
    "node-schedule": "^1.3.3",
    "nodemailer": "^6.9.1",
    "passport": "^0.4.1",
    "passport-mock-strategy": "^2.0.0",
    "passport-saml": "^3.2.4",
    "passport-saml-metadata": "^2.6.2",
    "reflect-metadata": "^0.1.13",
    "session-file-store": "^1.5.0",
    "strip-bom-buf": "^2.0.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typeorm": "^0.2.45",
    "typescript": "^4.9.5",
    "workerpool": "^6.4.0"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jest": "^26.0.24",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "4.3",
    "@typescript-eslint/parser": "4.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^29.5.0",
    "jest-date-mock": "^1.0.8",
    "jest-junit": "^14.0.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "source-map-support": "^0.5.21",
